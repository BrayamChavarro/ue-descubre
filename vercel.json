{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|html|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/admin-dashboard\\.js",
      "headers": {
        "Content-Type": "application/javascript; charset=UTF-8"
      }
    },
    {
      "src": "/admin\\.js",
      "headers": {
        "Content-Type": "application/javascript; charset=UTF-8"
      }
    },
    {
      "src": "/script\\.js",
      "headers": {
        "Content-Type": "application/javascript; charset=UTF-8"
      }
    },
    {
      "src": "/estudiantes\\.js",
      "headers": {
        "Content-Type": "application/javascript; charset=UTF-8"
      }
    },
    {
      "src": "/estadisticas\\.js",
      "headers": {
        "Content-Type": "application/javascript; charset=UTF-8"
      }
    },
    {
      "src": "/configuraciones\\.js",
      "headers": {
        "Content-Type": "application/javascript; charset=UTF-8"
      }
    },
    {
      "src": "/createAdmin\\.js",
      "headers": {
        "Content-Type": "application/javascript; charset=UTF-8"
      }
    },
    {
      "src": "/styles\\.css",
      "headers": {
        "Content-Type": "text/css; charset=UTF-8"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/admin$",
      "dest": "api/index.js"
    },
    {
      "src": "/admin/login",
      "dest": "api/index.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
