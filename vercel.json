{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/admin$",
      "dest": "api/index.js"
    },
    {
      "src": "/admin/login",
      "dest": "api/index.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
